{
  "name": "cs2-2d-demoviewer",
  "version": "1.0.0",
  "description": "Counter-Strike 2 Demo Viewer",
  "homepage": ".",
  "scripts": {
    "dev": "npm run parser:wasm && npm run webpack-dev",
    "webpack-dev": "webpack serve --mode development --config webpack.renderer.config.js",
    "build": "npm run parser:wasm && webpack --mode production --config webpack.renderer.config.js && npm run copy-static",
    "copy-static": "node scripts/copy-static.js",
    "parser:wasm": "node scripts/build-wasm.js"
  },
  "keywords": [
    "cs2",
    "demo",
    "viewer"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "20.0.0",
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.0",
    "css-loader": "7.1.4",
    "html-webpack-plugin": "5.6.6",
    "style-loader": "4.0.0",
    "ts-loader": "9.5.0",
    "typescript": "5.2.2",
    "webpack": "5.89.0",
    "webpack-cli": "5.1.0",
    "webpack-dev-server": "4.15.0"
  },
  "dependencies": {
    "fflate": "0.8.2",
    "fzstd": "0.1.1",
    "pixi.js": "8.16.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  }
}
